    "expo-router": "~6.0.23",
    "expo-status-bar": "~3.0.9",
    "nativewind": "^4.1.23",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-image-crop-picker": "^0.41.2",
    "react-native-reanimated": "~4.1.1",
